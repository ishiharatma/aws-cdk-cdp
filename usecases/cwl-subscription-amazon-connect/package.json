{
  "name": "cwl-subscription-amazon-connect",
  "version": "0.1.0",
  "bin": {
    "cwl-subscription-amazon-connect": "bin/cwl-subscription-amazon-connect.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:v": "cdk --version",
    "cdk:synth": "cdk synth --version-reporting false --path-metadata false --asset-metadata false --profile %npm_config_profile%",
    "cdk:diff:all": "cdk diff --all --version-reporting false --path-metadata false --asset-metadata false --profile %npm_config_profile%",
    "cdk:deploy:all": "cdk deploy --all --version-reporting false --path-metadata false --asset-metadata false --profile %npm_config_profile%",
    "cdk:destroy:all": "cdk destroy --all --profile %npm_config_profile%",
    "cdk:destroy:forceall": "cdk destroy --all --profile %npm_config_profile% --force"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "22.7.9",
    "aws-cdk": "^2.1000.0",
    "@aws-cdk/aws-lambda-python-alpha": "^2.182.0-alpha.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.180.0",
    "change-case-commonjs": "^5.4.4",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
